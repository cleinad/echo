{
  "name": "extension",
  "displayName": "Echo",
  "version": "0.0.1",
  "description": "explains anything you want to you.",
  "author": "dc",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.19",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "storage",
      "sidePanel"
    ],
    "host_permissions": [
      "https://*/*",
      "http://localhost/*"
    ],
    "externally_connectable": {
      "matches": [
        "http://localhost:3000/*",
        "https://*.vercel.app/*"
      ]
    },
    "action": {},
    "side_panel": {
      "default_path": "sidepanel.html"
    }
  }
}
